<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Airbnb tests getting reviews for apartment" thread-count="2" data-provider-thread-count="1">
