<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Airbnb tests category carousel testing" thread-count="2" data-provider-thread-count="1">
    <parameter name="browser" value="CHROME"></parameter>
    <parameter name="env" value="AIRBNB"></parameter>
    <parameter name="quit" value="yes"></parameter>
    <parameter name="category" value="Trending"></parameter>
    <parameter name="checkword" value="times last week"></parameter>
    <test name="Airbnb category carousel">
        <classes>
            <class name="tests.CategoryCarousel"></class>
        </classes>
    </test>
</suite>